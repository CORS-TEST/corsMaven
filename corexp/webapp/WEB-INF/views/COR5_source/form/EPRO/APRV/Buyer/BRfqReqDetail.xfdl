		case "BUserSrchPopup":
			if(!gfnIsNull(this.dsReturn.getColumn(0,"USER_ID"))){
				this.dsRfq.setColumn(0,"CHRGR_ID",this.dsReturn.getColumn(0,"USER_ID"));
				this.dsRfq.setColumn(0,"CHRGR_NM",this.dsReturn.getColumn(0,"USER_NM"));
