{
	if("APP_DOC_ID" == gfnGrdBindColId(obj, obj.getCellPos())){ 
// 		gfnSetTmprData("APP_DOC_ID"		,this.dsList.getColumn(e.row, "APP_DOC_ID"));
// 		gfnSetTmprData("GUBUN","RST");
