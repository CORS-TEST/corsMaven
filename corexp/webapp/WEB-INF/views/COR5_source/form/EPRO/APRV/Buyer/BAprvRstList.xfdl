		gfnModalPop(this, "BUserInfoPopup", "CPOP::BUserInfoPopup.xfdl", sArg, "", 550, 250);
	}else if(gfnGrdBindColId(obj, obj.getCellPos()) == "APRVR_NM" && !gfnIsNull(this.dsList.getColumn(e.row, "APRVR_NM"))){
		var nRow = obj.currentrow;
		var sArg = {
