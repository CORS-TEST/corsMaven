*/
this.div_2_btn_rfqTypeSelect_onclick = function(obj:Button, e:nexacro.ClickEventInfo)
{
	var sArg = { agv_id:""
			   , agv_nm:"" };
	
	gfnModalPop(this, "PopupDocType", "BTNDR::BTndrTypePopup.xfdl", sArg, "fnPopCallBack", 800, 400);
}
